    "    b = sns.barplot(world_region_mpi_mean_df.mean_mpi, world_region_mpi_mean_df.world_region)\n",
    "    plt.ylabel('')\n",
    "    plt.xlabel('MPI Average')\n",
    "    \n",
    "    plt.tight_layout(pad=0.0, w_pad=0.0, h_pad=2.0, rect=[0, 0, 0.95, 0.95])\n",
    "    \n",
    "    "
   ]
